<!DOCTYPE html>
<% var hwppejs = htmlWebpackPlugin.options.ejs %>
<html>
    <head>
        <title><%=hwppejs.title%></title>
        <link rel="stylesheet" href="/public/css/prime/prime.min.css"/>
        <!-- <link rel="stylesheet" href="/public/css/iconfont.css"/> -->
        <!-- <link rel="stylesheet" href="/public/css/style.css"/> -->
    </head>
    <body>
        <div id="vueapp" class="vueapp" :class="appclasses()" :style="appstyles()">
            <div class="adminpage wrapper">
                <div class="topbar">
                    <div class="topbar-left">
                        <c-navsbar
                        v-model="navs.current"
                        :list="navs.list"
                        @switch="navswitch"></c-navsbar>
                    </div>
                    <div class="topbar-right">
                        <c-searchbar
                        v-model="search.key"
                        @onsearch="search"></c-searchbar>

                        <c-icons-docker
                        :list="dockerlist"
                        @hover="dockerhover"
                        @click="dockerclick"></c-icons-docker>
                    </div>
                </div>
                <div class="content">
                    <transition name="fade">
                        <router-view></router-view>
                    </transition>
                </div>
            </div>

            <transition name="leftbar">
                <div class="leftbar" v-show="leftbar.show">
                    <c-leftpad></c-leftpad>
                </div>
            </transition>
            <transition name="leftbarsmall">
                <div class="leftbar small" v-show="leftbarsmall.show" >
                    <c-leftpadsmall></c-leftpadsmall>
                </div>
            </transition>
            <!-- <transition name="slide">
                <c-topbar :show="topbar.show"></c-topbar>
            </transition>
            <transition name="leftbar">
                <c-leftbar :show="leftbar.show"></c-leftbar>
            </transition>
            <transition name="leftbarsmall">
                <c-leftbar :show="leftbarsmall.show" class="small"></c-leftbar>
            </transition> -->
        </div>
    </body>
</html>
